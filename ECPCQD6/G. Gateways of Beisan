#include <bits/stdc++.h>
using namespace std;

void FastIO(){ios_base::sync_with_stdio(false);cin.tie(nullptr);}

void solve(){
    int n; cin >> n;
    string s; cin >> s;
 
    bool get = false;
 
    for(int i = 0; i <= n - 2; i++){
        if(s[i] == s[i+1] || s[i] == s[i+2] || s[i+1] == s[i+2]){
            cout << "YES\n";
            get = true;
            break;
        }
    }
    if(!get) cout << "NO\n";
}

int main()
{
    FastIO();
    solve();
    return 0;
}
