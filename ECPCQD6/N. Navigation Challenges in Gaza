#include<bits/stdc++.h>
using namespace std;

void FastIO(){ios_base::sync_with_stdio(false);cin.tie(nullptr);}

void solve(){
    int n; cin >> n;

    int arr[n];
    arr[0] = 1;
    arr[1] = 2;

    for(int i = 2; i < n; i++){
        arr[i] = (arr[i-1] % 10) * (arr[i-2] % 10);
    }

    cout << arr[n-1] << endl;

}
int main()
{
    FastIO();
    solve();
    return 0;
}
